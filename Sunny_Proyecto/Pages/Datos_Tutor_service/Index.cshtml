@page
@model Sunny_Proyecto.Pages.Datos_Tutor_service.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Datos del Tutor</h1>

<p>
    <a asp-page="Crear">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Nombre_tutor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Apellido_Paterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Apellido_Materno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Fecha_Nacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Nacionalidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Ocupacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Ingreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Datos_Tutor[0].Telefono)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Datos_Tutor) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_tutor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido_Paterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido_Materno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Nacimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nacionalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocupacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ingreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Editar</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Detalles</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Borrar</a>
            </td>
        </tr>
}
    </tbody>
</table>
